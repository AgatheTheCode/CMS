{% extends 'base.twig' %}
{% block page %}
	{% for element in page %}
		{% if element.type == 'text' %}
			{% include 'text.twig' %}
		{% elseif element.type == 'img' %}
			{% include 'image.twig' %}
		{% elseif element.type == 'vid' %}
			{% include 'video.twig' %}
		{% elseif element.type == 'link' %}
			{% include '???.twig' %}
		{% endif %}
	{% endfor %}
{% endblock %}