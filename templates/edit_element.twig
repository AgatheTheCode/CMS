{% extends 'base.twig' %}

{% block page %}
	<h6>Créer une page</h6>
	<form action="index.php?page=auteur&action=update" method="post">
		<input type="texte" readonly name="id" value="{{ page.id }}">
		<input type="text" name="nom" placeholder="Saisir un nom" value="{{ page.nom }}">
		<input type="submit" value="Créer">
	</form>
{% endblock %}